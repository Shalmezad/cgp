#!/usr/bin/env python3

from cgp.improbed import BrainBuilder, Config
from cgp.problems import GlassProblem


problem = GlassProblem()

config = Config(
    num_inputs=[problem.numInputs()],
    num_outputs=[problem.numOutputs()]
)
brain_builder = BrainBuilder(config)
brain = brain_builder.build()

for _ in range(10):
    brain = brain.update()

ann = brain.extractANN(1, 1)
